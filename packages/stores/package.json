{
  "name": "@prometheus-fe/store",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build:js": "tsup src/index.ts --format cjs,esm --tsconfig tsconfig.json",
    "build:dts": "tsc --emitDeclarationOnly --project tsconfig.json",
    "build": "pnpm run build:js && pnpm run build:dts",
    "dev": "tsup src/index.ts --format cjs,esm --watch --tsconfig tsconfig.json",
    "clean": "rm -rf dist .tsbuildinfo"
  },
  "dependencies": {
    "zustand": "^5.0.0",
    "@prometheus-fe/types": "workspace:*"
  },
  "devDependencies": {
    "tsup": "^8.0.2",
    "typescript": "^5",
    "@prometheus-fe/typescript-config": "workspace:*"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  }
}


